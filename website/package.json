    ]
  },
  "devDependencies": {
    "dumi": "^1.0.13",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@lifhcp/rc-grid": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
